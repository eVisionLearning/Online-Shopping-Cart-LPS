    <script>
        var cartApp = Vue.createApp({
            data() {
                return {
                    cartList: [],
                    isLoggedIn: @Json.Serialize(_db.GetLoggedInUser() != null)
                }
            },
            methods:{
                addToCart: function(id){
                    if(!this.isLoggedIn){
                        window.location.assign('/login');
                    }
                    $.post('/Cart/AddOrUpdateCart', { id: id }, function(res){
                        if(res.status){
                            cartApp.cartList = res.data;
                        }
                    })
                },
            }
        }).mount('#cartApp');
    </script>
}