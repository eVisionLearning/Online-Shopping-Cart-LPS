@inject AppDbContext _db
@{
    ViewData["Title"] = "Home Page";
    var products = _db.Products.Select(m => new
    {
        m.Id,
        m.Name,
        ImageUrl = m.Images.OrderBy(s => s.Rank).Select(s => s.URL).FirstOrDefault(),
        m.Price,
        CategoryName = m.Category.Name
    }).ToList();
}

